<h3 class="text-center">
  Produce You've Requested
</h3>
<div class="row">
  <div class="list-group col-md-6">
    {{#eachPropertyColumn myDemand 1 2}}
      <a href="#" class="list-group-item supply-filter active" data-name="{{property}}">
        {{property}} <span class="badge">{{value.length}}</span>
      </a>
    {{/eachPropertyColumn}}
    </div>
    <div class="list-group col-md-6">
    {{#eachPropertyColumn myDemand 2 2}}
      <a href="#" class="list-group-item supply-filter active" data-name="{{property}}">
        {{property}} <span class="badge">{{value.length}}</span>
      </a>
    {{/eachPropertyColumn}}
  </div>
</div>

