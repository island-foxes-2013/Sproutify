<h3 class="text-center">
  Produce You're Growing
</h3>
<div class="row">
  <div class="list-group col-xs-6">
    {{#eachPropertyColumn myGrowing 1 2}}
      <a href="#" class="list-group-item demand-filter active" data-name="{{property}}">
        {{property}} <span class="badge">{{value.length}}</span>
      </a>
    {{/eachPropertyColumn}}
    </div>
    <div class="list-group col-xs-6">
    {{#eachPropertyColumn myGrowing 2 2}}
      <a href="#" class="list-group-item demand-filter active" data-name="{{property}}">
        {{property}} <span class="badge">{{value.length}}</span>
      </a>
    {{/eachPropertyColumn}}
  </div>
</div>

